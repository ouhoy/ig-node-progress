<service>
	<id>updateigbio.exe</id>
	<name>Update IG Bio</name>
	<description>Node.js service that updates my IG bio everyday, posting a progress bar</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>C:\Users\abdel\Desktop\DEV\ig-node-progress\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\Users\abdel\Desktop\DEV\ig-node-progress\server.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>Update IG Bio wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>OUHOY</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\Users\abdel\Desktop\DEV\ig-node-progress</workingdirectory>
</service>